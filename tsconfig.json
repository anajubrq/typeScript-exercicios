{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "rootDir": "questoes-facil/typeScript",  // Ajuste conforme o local correto
      "outDir": "questoes-facil/javaScript",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": [
      "questoes-facil/typeScript/**/*"
    ]
  }
  
  